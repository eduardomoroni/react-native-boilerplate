{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "scripts/pre-install.sh",
    "postinstall": "scripts/post-install.sh",
    "clean": "scripts/clear-cache.sh",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "info": "node node_modules/react-native/local-cli/cli.js info",
    "test": "jest",
    "ios": "react-native run-ios",
    "preandroid": "scripts/android-emulator.sh start",
    "android": "react-native run-android",
    "ios:build": "bundle exec fastlane ios build",
    "android:build": "bundle exec fastlane android build",
    "precommit": "lint-staged",
    "flow": "flow",
    "flow:coverage": "flow coverage",
    "flow:install-libdefs": "flow-typed install",
    "prettier": "prettier --write 'src/**/*.js'",
    "check-code-quality": "npm run flow"
  },
  "dependencies": {
    "data.maybe": "1.2.2",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-config": "0.11.5",
    "react-native-firebase": "4.2.0",
    "react-native-i18n": "2.0.12",
    "react-native-navigation": "2.0.2295",
    "react-native-vector-icons": "4.6.0",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-form": "7.3.0",
    "redux-logger": "3.0.6"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-jest": "22.4.3",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react-native": "4.0.0",
    "flow-bin": "0.67.1",
    "flow-typed": "2.4.0",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "lint-staged": "7.1.1",
    "prettier": "1.12.1",
    "react-test-renderer": "16.3.1",
    "redux-devtools": "3.4.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "engines": {
    "node": "10.1.0",
    "npm": "6.0.1"
  }
}
